{
  "name": "etherscan-api",
  "version": "2.0.2",
  "description": "API to etherscan with a simple interface",
  "main": "index.js",
  "scripts": {
    "test": "mocha -t 20000",
    "posttest": "npm run lint",
    "lint": "jshint lib test",
    "docs": "jsdoc -c ./jsdoc.json -R ./Readme.md -u ./docs",
    "bundle": "browserify index.js -s etherscanApi -o dist/bundle.js",
    "preversion": "npm run lint",
    "pages": "node ./scripts/publish-gh.js",
    "postversion": "git push && git push --tags && npm run pages",
    "changelog": "rm CHANGELOG.md && changelog https://github.com/sebs/etherscan-api all > CHANGELOG.md && git add . && git commit . -m changelog",
    "build": "npm run test && npm run docs"
  },
  "keywords": [
    "ethereum",
    "etherscan",
    "api",
    "transaction",
    "rest"
  ],
  "repository": {
    "type": "git",
    "url": "git+https://github.com/sebs/etherscan-api.git"
  },
  "author": "",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/sebs/etherscan-api/issues"
  },
  "homepage": "https://github.com/sebs/etherscan-api#readme",
  "devDependencies": {
    "browserify": "^13.1.0",
    "chai": "^3.5.0",
    "gh-pages": "^0.11.0",
    "jsdoc": "^3.4.0",
    "jshint": "^2.9.3",
    "mocha": "^3.0.2",
    "watch": "^0.19.2",
    "changelog": "^1.0.7"
  },
  "dependencies": {
    "restify": "^4.1.1"
  }
}
